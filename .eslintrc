{
    "extends": "airbnb",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
      "node": true,
      "browser":true,
      "es6":true
    },
    "rules":{
      "linebreak-style": "off",
      "comma-dangle": ["error", "never"],
      "no-unused-vars":[
        "error",
        {
          "vars":"local",
          "args":"none"
        }
      ],
      "react/jsx-filename-extension":0,
      "react/prop-types":0
    }
  }
  